{
  "name": "moded12/appliancess",
  "description": "E-commerce store with PayPal and Stripe payment integration",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "stripe/stripe-php": "^13.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "includes/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
